{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "date"
    },
    "total_price": {
      "required": false,
      "min": 0,
      "max": 0,
      "default": 0,
      "type": "float"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "payment_detail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::payment-detail.payment-detail",
      "mappedBy": "order"
    }
  }
}
